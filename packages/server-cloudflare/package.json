{
	"name": "@pigeonposse/api-cloudflare",
	"version": "0.0.0",
	"private": true,
	"type": "module",
	"exports": {
		"./schema/": "./dist/schema/",
		"./types": "./dist/types.d.ts"
	},
	"types": "./dist/types.d.ts",
	"files": [
		"dist"
	],
	"scripts": {
		"build": "pnpm build:types && pnpm build:schema",
		"build:schema": "node schema.js",
		"build:types": "vite build",
		"cf-typegen": "wrangler types",
		"dev": "wrangler dev --test-scheduled",
		"dev-remote": "wrangler dev --remote --test-scheduled",
		"release": "pnpm build && wrangler deploy"
	},
	"dependencies": {
		"@collectium/core": "0.2.2",
		"@pigeonposse/config": "workspace:*"

	},
	"devDependencies": {

		"@cloudflare/workers-types": "4.20250124.3",
		"wrangler": "3.106.0"
	}
}
