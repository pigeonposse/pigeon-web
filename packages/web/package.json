{
	"name": "@pigeonposse/web-2024",
	"version": "0.0.1",
	"repository": {
		"type": "git",
		"url": "https://github.com/pigeonposse/pigeon-web"
	},
	"type": "module",
	"files": [
		"dist"
	],
	"scripts": {
		"build": "vite build",
		"build-assets": "dovenv -c ../../.dovenv/main.js docs generate-assets --flag=\"--preset minimal\" --flag=\"static/logo.png\"",
		"check": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json",
		"check:watch": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json --watch",
		"dev": "pnpm --parallel \"/dev:.*/\"",
		"dev:server": "pigeonposse-api",
		"dev:web": "vite dev",
		"preview": "vite preview",
		"preview-dist": "pnpx serve dist/web",
		"test": "pnpm test:e2e && pnpm test:unit",
		"test:e2e": "playwright test",
		"test:unit": "vitest run src --passWithNoTests"
	},
	"devDependencies": {
		"@fortawesome/free-brands-svg-icons": "6.7.2",
		"@fortawesome/free-solid-svg-icons": "6.7.2",
		"@pigeonposse/api-2024": "workspace:*",
		"@pigeonposse/api-client-2024": "workspace:*",
		"@sveltejs/adapter-static": "3.0.8",
		"@sveltejs/kit": "2.17.1",
		"@sveltejs/vite-plugin-svelte": "5.0.3",
		"@tauri-apps/api": "2.2.0",
		"@tauri-apps/cli": "2.2.7",
		"autoprefixer": "10.4.20",
		"eslint-plugin-svelte": "2.46.1",
		"flowbite": "3.1.1",
		"flowbite-svelte": "0.47.4",
		"marked": "15.0.6",
		"postcss": "8.5.1",
		"svelte": "5.19.6",
		"svelte-check": "4.1.4",
		"svelte-fa": "4.0.3",
		"svelte-portal": "2.2.1",
		"sveltekit-i18n": "2.4.2",
		"tailwindcss": "3.4.17"
	}
}
