	},
	"type": "module",
	"workspaces": [
		"packages/*"
	],
	"scripts": {
		"+": "run() { pnpm -F '@*/$1' ${@:2}; }; run $@",
		"++": "run() { pnpm -F $1 ${@:2}; }; run $@",
		".": "dovenv",
		"build": "pnpm + api-config-2024 build && pnpm + api-2024 build && pnpm + api-client-2024 build && pnpm + api-web-2024 build",
		"build-container": "docker compose -f packages/web/docker-compose.yml",
		"test": "pnpm run --sequential \"/test:.*/\"",
		"test:check-ws": "dovenv check --silent",
		"test:pkgs": "pnpm -F '*' run check && pnpm -F '*' run test",
		"test:transform-ws": "dovenv transform --silent"
	},
	"devDependencies": {
		"@changesets/changelog-github": "0.5.0",
		"@dovenv/core": "1.3.2",
		"@dovenv/theme-pigeonposse": "1.3.2",
		"@playwright/test": "1.50.1",
		"@sveltejs/eslint-config": "^8.1.0",
		"@types/node": "22.13.0",
		"internal-ip": "8.0.0",
		"tslib": "2.8.1",
		"typescript": "5.7.3",
		"unbuild": "3.3.1",
		"vite": "6.0.11",
		"vite-plugin-dts": "4.5.0",
		"vitest": "3.0.5"
	},
	"packageManager": "pnpm@9.1.1",
	"engines": {
		"node": ">=20.0.0",
		"pnpm": ">=8.10.0"
	},
	"extra": {
		"id": "pigeon-web",
